<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CornerButton">
        <attr name="cbCornerType">
            <enum name="cut" value="1" />
            <enum name="round" value="0" />
        </attr>

        <attr name="cbText" format="string" />
        <attr name="cbTextColor" format="reference|color" />
        <attr name="cbTextSize" format="integer" />
        <attr name="cbRadius" format="dimension" />
        <attr name="cbTopLeftRadius" format="dimension" />
        <attr name="cbTopRightRadius" format="dimension" />
        <attr name="cbBottomLeftRadius" format="dimension" />
        <attr name="cbBottomRightRadius" format="dimension" />
        <attr name="cbFillColor" format="reference|color"/>
        <attr name="cbStrokeColor" format="reference|color"/>
        <attr name="cbStrokingWidth" format="dimension"/>
        <attr name="cbBackgroundDrawable" format="reference|color" />
        <attr name="cbOptionsDrawable" format="reference" />
        <attr name="cbEnabled" format="boolean" />
    </declare-styleable>
</resources>